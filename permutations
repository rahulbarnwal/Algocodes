#include<iostream>
#include<algorithm>
#include<string>

using namespace std;

void permute(string s,int l,int r){
  if(l==r)cout<<string<<endl;
  else
    int i;
    for(i=l;i<=r;i++){
      swap(s,a+l,a+i);
      permute(s,l+1,r);
      swap(s,a+l,a+i);
     }
  }   

int main(){
 int t;
 cin>>t;
 int i,n;
 string s;
 for(i=0;i<t;i++){
   cin>>s;
   n=s.length();
   permute(s,0,n-1);
   }
 return 0;
 }  
   
